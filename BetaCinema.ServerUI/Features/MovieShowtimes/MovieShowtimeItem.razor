@inherits MovieShowtimeItemBase

<MudCard Elevation="0" Style="background-color:transparent">
    <div class="showtime-item">
        <div class="@ShowtimeClass" onclick="@ShowConfirmShowtimeDialog">
            @ShowtimeData.StartTime.Value.ToString("HH:mm")
        </div>
        <span class="seats-unsold">@($"{unsoldSeats} ghế trống")</span>
    </div>
</MudCard>
